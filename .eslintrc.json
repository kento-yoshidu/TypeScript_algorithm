{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "quotes": ["warn", "double"],
    "semi": ["warn", "never"],
    "max-len": "off",
    "comma-dangle": ["warn", "never"],
    "object-curly-spacing": ["warn", "always"],
    
  }
}
